function _init()
 ship = spawn("ship", { x = 60,  y = 60 })
 if (two_player_mode) then mf = spawn("mf", { x = 40, y = 40 }) end
 bullets = {}
 enemies = {}
 explosions = {}
 stars = {}
 for i=1,100 do
  add(stars, spawn("star", {x=rndi(180), y=rndi(180), dy=rnd(2)}))
 end
 bound = {x1 = 2, x2 = 128, y1 = 0, y2 = 120}
 ammos = {}
 ammos[0] = {
  type = "bomb",
  n = 5,    -- qty
  t = 0,    -- last time fired
  dr = 200,   -- replenish rate
  df = 30,  -- fire rate
  mx = 10,  -- max
 }
 ammos[1] = {
  type = "bullet",
  n = 10,
  t = 0,
  dr = 25,
  df = 5,
  mx = 20,
 }
 ammos[2] = {
  type = "falconbullet",
  n = 10,
  t = 0,
  dr = 25,
  df = 5,
  mx = 20,
 }
 ammos[3] = {
  type = "falconl",
  type2 = "falconr",
  n = 20,
  t = 0,
  dr = 30,
  df = 10,
  mx = 20,
 }
end


function replenish()
 create_enemy()

 -- replenish ammo
 for k,a in pairs(ammos) do
--  local a = ammos[k]
  if ((t % a.dr) == 0 and a.n < a.mx) then
    a.n += 1
  end
 end
end


function create_enemy()
 if ((t % info.tie.dr) == 0) then
  e = spawn("tie", { x=rndi(100), y=20 })
  add(enemies, e)
 end
end

function continue_exploding()
  local to_kill = {}
  for e in all(explosions) do
   if e.spi >= #e.sps then
      del(explosions, e)
   else
      if (t%3) == 0 then
       e.spi = e.spi + 1
       e.sp = e.sps[e.spi]
      end
   end
  end
  for e in all(to_kill) do
   del(dead_enemies, e)
  end
end

function killed(b, tbl)
 local expl = {}
 for de in all(tbl) do
  sfx(2)
  score.good += 1
  info[de.type].dr -= 2
  if (info[de.type].dr < 3) then
    info[de.type].dr = 3
  end
  info[de.type].dy += 0.1
  del(enemies, de)
  del(bullets, b)
  local props = { x = de.x, y = de.y }
  if b.explosion then
   props.explosion = {}
   merge(props.explosion, b.explosion)
  end
  add(expl, spawn("explosion", props))
 end
 return expl
end


function _update()
 t += 1
 animate(ship)
 if (two_player_mode) then animate(mf) end
 replenish()
 --
 for b in all(bullets) do
  moveobj(b)
  -- just remove ones that go off screen
  if not in_bound(b, bound) then
   del(bullets, b)
  end
  -- check for collisions
  local newly_died = collide_any(b, enemies)
  if next(newly_died) == nil then
   -- nothing
  else
   merge(explosions, killed(b, newly_died))
   if (b.explosion) then
    printh("checking for extra big explosion " .. #newly_died)
    local more_exploded = collide_any_explosion(b, enemies)
    if next(more_exploded) != nil then
      printh(#more_exploded .. "more died!" )
      merge(explosions, killed(b, more_exploded))
    end
   end
  end
 end

 continue_exploding()

 for e in all(enemies) do
  -- printh(e.id .. ": [" .. e.x .. ", " .. e.y .. "]")
  moveobj(e)
  if not in_bound(e, bound) then
    score.bad += 1
    info[e.type].dr = min(info[e.type].dr + 1, 50)
    info[e.type].dy = max(info[e.type].dy - 0.3, 0.5)
    del(enemies, e)
  end
 end

 if ((t % info.star.dr) == 0) then
  add(stars, spawn("star", {x = rndi(180), y = rndi(3), dy=rnd(2) }))
 end

 for s in all(stars) do
  s.y += s.dy
  s.x += dx
  if not in_bound(s, bound) then del(stars, s) end
 end

 -- buttons
 if btn(⬆️) then
  ship.dy = min(0, ship.dy) - 0.1
  ship.y+=ship.dy
 end
 if btn(⬇️) then
  ship.dy = max(0, ship.dy) + 0.1
  ship.y+=ship.dy
 end
 if btn(⬅️) then
  ship.dx = min(0, ship.dx) - 0.1
  ship.x += ship.dx
 end
 if btn(➡️) then
  ship.dx = max(0, ship.dx) + 0.1
  ship.x+=ship.dx
 end
 constrain(ship, bound)

 if (two_player_mode) then
   if btn(⬆️, 1) then
    mf.dy = min(0, mf.dy) - 0.1
    mf.y+=mf.dy
   end
   if btn(⬇️, 1) then
    mf.dy = max(0, mf.dy) + 0.1
    mf.y+=mf.dy
   end
   if btn(⬅️, 1) then
    mf.dx = min(0, mf.dx) - 0.1
    mf.x += mf.dx
   end
   if btn(➡️, 1) then
    mf.dx = max(0, mf.dx) + 0.1
    mf.x+=mf.dx
   end
   constrain(mf, bound)
 end


 if btn(🅾️) then fire(0) end
 if btn(❎) then fire(1) end
 if btn(❎, 1) then fire(2) end
 if btn(🅾️, 1) then fire(3) end
end

function _draw()
 cls()
 for s in all(stars) do
  pset(s.x, s.y, s.c)
 end
 for b in all(bullets) do
  spr(b.sp, flr(b.x), flr(b.y))
 end
 for e in all(enemies) do
  spr(e.sp, flr(e.x), flr(e.y))
 end
 for de in all(explosions) do
  draw_explosion(de)
 end
 print(score.good or 0, 40, 120, 6)
 print(score.bad  or 0, 60, 120, 8)
 print(ammos[0].n .. "/" .. ammos[0].mx, 0,120, 7)
 print(ammos[1].n .. "/" .. ammos[1].mx, 90,120, 7)
 spr(ship.sp, ship.x, ship.y)
 if (two_player_mode) then spr(mf.sp, mf.x, mf.y) end
end

function draw_explosion(ex)
  if (ex.explosion) then
   local x1 = ex.x + ex.explosion.x1
   local x2 = ex.x + ex.explosion.x2
   local y1 = ex.y + ex.explosion.y1
   local y2 = ex.y + ex.explosion.y2
   for b = y1, y2, 8 do
    for a = x1, x2, 8 do
     spr(ex.sp, a, b)
    end
   end --
  else
   spr(ex.sp, flr(ex.x), flr(ex.y))
  end
end


